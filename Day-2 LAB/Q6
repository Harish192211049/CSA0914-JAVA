public class MethodOverloading {

    // Method with two int parameters
    public int sum(int a, int b) {
        return a + b;
    }

    // Overloaded method with three int parameters
    public int sum(int a, int b, int c) {
        return a + b + c;
    }

    // Overloaded method with two double parameters
    public double sum(double a, double b) {
        return a + b;
    }

    // Overloaded method with int and double parameters
    public double sum(int a, double b) {
        return a + b;
    }

    public static void main(String[] args) {
        MethodOverloading obj = new MethodOverloading();

        // Call method with two int parameters
        System.out.println("Sum of 10 and 20: " + obj.sum(10, 20));

        // Call overloaded method with three int parameters
        System.out.println("Sum of 10, 20, and 30: " + obj.sum(10, 20, 30));

        // Call overloaded method with two double parameters
        System.out.println("Sum of 10.5 and 20.5: " + obj.sum(10.5, 20.5));

        // Call overloaded method with int and double parameters
        System.out.println("Sum of 10 and 20.5: " + obj.sum(10, 20.5));
    }
}
