package level1;

public class Armstrong {

	public static void main(String[] args) {
		int n=134;
		int rem,res=0,orinum;
		orinum=n;
		while(orinum!=0) {
			rem=orinum%10;
			res+=rem*rem*rem;
			n/=10;
		}
		if(n==res) {
			System.out.println("Armstrong number");
		}
		else {
			System.out.println("Not an Armstrong number");
		}

	}

}
