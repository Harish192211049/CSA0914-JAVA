public class SimpleMultiplicationTable {
    public static void main(String[] args) {
        int number = 5; // Change this to 10 if you want the table for 10

        // Loop to display the multiplication table
        for (int i = 1; i <= 10; i++) {
            System.out.println(number + " x " + i + " = " + (number * i));
        }
    }
}
